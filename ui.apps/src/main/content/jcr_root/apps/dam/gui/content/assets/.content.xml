<?xml version="1.0" encoding="UTF-8"?>
<jcr:root xmlns:jcr="http://www.jcp.org/jcr/1.0"
          xmlns:granite="http://www.adobe.com/jcr/granite/1.0" xmlns:nt="http://www.jcp.org/jcr/nt/1.0"
          xmlns:cq="http://www.day.com/jcr/cq/1.0"
          xmlns:sling="http://sling.apache.org/jcr/sling/1.0"
          jcr:primaryType="cq:Page">
    <jcr:content jcr:primaryType="nt:unstructured">
        <actions jcr:primaryType="nt:unstructured">
            <selection jcr:primaryType="nt:unstructured">
                <editasset jcr:primaryType="nt:unstructured">
                    <granite:rendercondition
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="granite/ui/components/coral/foundation/renderconditions/simple"
                            expression="{Boolean}false"/>
                </editasset>
                <moveasset jcr:primaryType="nt:unstructured">
                    <granite:rendercondition
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="cq/gui/components/siteadmin/admin/listview/columns/grouprendercondition"
                            groups="[administrators,AEM-gen-content-administratoren]"/>
                </moveasset>
                <delete jcr:primaryType="nt:unstructured">
                    <granite:rendercondition
                            jcr:primaryType="nt:unstructured"
                            sling:resourceType="cq/gui/components/siteadmin/admin/listview/columns/grouprendercondition"
                            groups="[administrators,AEM-gen-content-administratoren]"/>
                </delete>
            </selection>
        </actions>
    </jcr:content>
</jcr:root>
